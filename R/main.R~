main <-
function(matrix,scores=1,method='r1',sims=1000){
	require(lattice)
	mat=OrderMatrix(matrix,scores=scores)
	comat=coherence.null(mat,method=method,sims=sims,scores=scores)
	boundmat=BoundaryClump(mat)
  ranmat=Range(mat,method=method,sims=sims,scores=scores)
	ret=list(Matrix=mat, Coherence=comat, Range=ranmat, Boundary=boundmat)
	return(ret)
}
